# API Keys Configuration
# Copy this file to .env and add your actual API keys

# Groq AI (for sentiment analysis)
VITE_GROQ_API_KEY=your_groq_api_key_here

# CoinGecko (for market data) 
VITE_COINGECKO_API_KEY=your_coingecko_api_key_here

# WhaleAlert (for whale tracking)
VITE_WHALEALERT_API_KEY=your_whalealert_api_key_here

# Etherscan (for blockchain data)
VITE_ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Bitquery (for cross-chain data)
VITE_BITQUERY_ACCESS_TOKEN=your_bitquery_token_here

# Alpaca (for paper trading)
VITE_ALPACA_API_KEY=your_alpaca_key_here
VITE_ALPACA_SECRET_KEY=your_alpaca_secret_here

# Supabase (for persistence)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Encryption key for secure storage
VITE_ENCRYPTION_KEY=your_encryption_key_here